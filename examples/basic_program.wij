module main;

use core:fmt;

type Color = Red | Green | Blue int;

type Test = {
    x: int
}

fn fib(n: int) -> int {
    if n == 0 or n == 1 {
        return n;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

fn main() {
    let x: int = 5;
    let y: int = fib(x);

    let t: Test = Test { x = 1 };

    let c: Color = Blue 1;

    x = 1;
    t.x = 2;
    println(int_to_string(y));
}
